<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/login_setting_selected_bg"/>
            <corners android:radius="@dimen/mdp_6"/>
            <stroke
                android:width="@dimen/mdp_0_4"
                android:color="@color/login_setting_defulit" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/login_setting_selected_bg"/>
            <corners android:radius="@dimen/mdp_6"/>
            <stroke
                android:width="@dimen/mdp_0_4"
                android:color="@color/login_setting_defulit" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/all_background_color"/>
            <corners android:radius="@dimen/mdp_6"/>
            <stroke
                android:width="@dimen/mdp_0_4"
                android:color="@color/login_setting_defulit" />
        </shape>
    </item>
</selector>
